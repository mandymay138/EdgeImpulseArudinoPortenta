{
    "version": 1,
    "info": {
        "title": "Image Block",
        "author": "Mandy Wong",
        "description": "Extract Face From Image",
        "name": "Mandy Custom block",
        "cppType": "custom_block",
        "preferConvolution": false,
        "visualization": "dimensionalityReduction",
        "experimental": false,
        "latestImplementationVersion": 1
    },
    "parameters": [
        {
            "group": "Channels",
            "items": [
                {
                    "name": "Image Channels",
                    "value": "Grayscale",
